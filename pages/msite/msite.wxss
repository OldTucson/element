/* pages/msite/msite.wxss */
.types-swiper {
  background: #fff;
  margin-bottom: 20rpx;
}
.types {
  display: flex;
  flex-wrap: wrap;
}
.type {
  width: 25%;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.type image {
  width: 90rpx;
  height: 90rpx;
  margin-bottom: 10rpx;
}
.shops{
  margin-top: 20rpx;
  padding: 20rpx;
  border: 1px solid #eee;
  
}
.shops .shops-title{
  font-size: 30rpx;
  color: #999;
  border-bottom: 1px solid #eee;
}
.shops .shop{
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1px solid #eee;
  font-size: 20rpx;
  color: #999;
}
.shops .shop .shop-pic>image{
  height: 130rpx;
  width: 130rpx;
}
.shops .shop .shop-desc{
  flex: 7.5;
  display: flex;
  flex-flow: column wrap;
  justify-content: space-between;
  height: 130rpx;
  margin-left: 20rpx;
}
.shops .shop .shop-desc>view{
  display: flex;
  justify-content: space-between;
}
.shops .shop .shop-desc .shop-desc-mid .rate-count{
  margin: 0 16rpx;
}
.shops .shop .shop-desc .shop-desc-mid .rate{
  position: relative;
  display: inline-block;
  height: 16rpx;
  width:100rpx;
}
.shops .shop .shop-desc .shop-desc-mid .rate image{
  position: absolute;
  left: 0;
  top: 0;
  width: 100rpx;
  height: 16rpx;
}
.shops .shop .shop-desc .shop-desc-mid .rate .star{
  position: absolute;
  left: 0;
  top: 0;
  width: 100rpx;
  height: 16rpx;
  background: url("data:image/png;base64,") no-repeat left center / 100rpx 16rpx;

}
.shops .shop .shop-desc .shop-desc-top .brand{
  font-size: 24rpx;
  color: #333;
  background: #ffd930;
  font-weight: bolder;
  display: inline-block;
  padding: 2rpx;
  margin-right: 10rpx;
}
.shops .shop .shop-desc .shop-desc-top .name{
  font-size: 30rpx;
  font-weight: bolder;
  color: #333;
}
.shops .shop .shop-desc .shop-desc-mid .rate-count{
  color: #ff6000;
}
.shops .shop .shop-desc .shop-desc-mid .method{
  font-size: 18rpx;
  display: inline-block;
  padding: 2rpx;
}
.shops .shop .shop-desc .shop-desc-mid .method1{
  color: white;
  background: #3190e8;
  
}
.shops .shop .shop-desc .shop-desc-mid .method2{
  padding: 0 2rpx;
  margin-left: 2rpx;
  color: #3190e8;
  border: 1px solid #3190e8;
  
}
.shops .shop .shop-desc .shop-desc-bot .time{
  color: #3190e8;
}